      title="GeeTest v3 Image"
      imageSrc="/icons/geetest.png"
      altText="GeeTest v4"
      link="/products/imagetasks/geetestv4/"
    />
    <CustomCard
      title="GeeTest v3 Token"
      imageSrc="/icons/geetest.png"
      altText="GeeTest v4"
      link="/products/imagetasks/geetestv4/"
    />
    <CustomCard
      title="AWSWaf Image"
      imageSrc="/icons/awswaf.png"
      altText="AWSWAF"
      link="/products/imagetasks/awswaf/"
    />
    <CustomCard
      title="Binance Image"
      imageSrc="/icons/awswaf.png"
      altText="AWSWAF"
      link="/products/imagetasks/awswaf/"
    />
    <CustomCard
      title="Tiktok Image"
      imageSrc="/icons/awswaf.png"
      altText="AWSWAF"
      link="/products/imagetasks/awswaf/"
    />
    <CustomCard
      title="mtCAPTCHA"
      imageSrc="/icons/mtcaptcha.png"
      altText="Cloudflare Turnstile"
      link="/products/imagetasks/mtcaptcha"
    />
